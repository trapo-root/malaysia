                          src="{{ product.featured_media.preview_image | img_url }}"
                          alt="{{ product.title }}"
                          srcset="//bellavitaluxury.co.in/cdn/shop/files/1_402bde70-7bac-4e1f-a698-a374f6d5f12a.jpg?v=1714555988&amp;width=375 375w, //bellavitaluxury.co.in/cdn/shop/files/1_402bde70-7bac-4e1f-a698-a374f6d5f12a.jpg?v=1714555988&amp;width=500 500w"
                          width="500"
                          height="500"
                          loading="lazy"
                          fetchpriority="low"
                          sizes="100vw"
                          class="grid-view-item__image"
                        >
                      -->
                      {% render 'image-element',
                        image: product.featured_media.preview_image,
                        alt: alt_text,
                        object_fit: settings.align_height,
                        max_height: settings.collection_height
                      %}
                    </a>
                  </div>
                </div>
                <div class="product_grid_items">
                  <h3 class="card-information__text h5">
                    <a href="javascript:void(0);">{{ product.title }}</a>
                  </h3>
                  <!-- Custom code for product rating -->

                  <!-- Custom code for product rating ends here -->
                </div>

                <div id="checkboxes">
                  <p class="line-item-property__field add-to-box">
                    <label>
                      <input type="hidden" autocomplete="off" name="properties[_ITEM1]" value="No">
                      <input
                        id="{{ product.handle }}"
                        class="button"
                        type="checkbox"
                        data-id-products="tab_1_product"
                        data-id="tab_1_product_{{ forloop.index }}"
                        autocomplete="off"
                        name="properties[_ITEM1]"
                        value="
                          {{ product.selected_or_first_available_variant.title }}
                          SKU: {{ product.selected_or_first_available_variant.image.sku }}
                        "

                      ><span></span>
                    </label>
                  </p>
                </div>
              </div>
            </div>
          </li>
        {% endfor %}
