<style>
      .tab-menu ul.tab-items {
  	display: flex;
  	justify-content: center;
  	align-items: center;
  	margin: 0
  }

  .tab-menu ul li {
  	list-style-type: none;
  	display: inline-block
  }

  .tab-menu ul li a {
  	text-decoration: none
  }

  .tab-content {
  	display: none
  }

  .active-tab-content {
  	display: block
  }

  a.active-tab .sticky-header-bubble h5,
  a.active-tab .sticky-header-bubble p,
  a.active-tab .sticky-header-bubble:after {
  	color: #000
  }

  a.active-tab .sticky-header-bubble:after {
  	background: #d5e0ea
  }

  a.active-tab .sticky-header-bubble:before {
  	background: #a9a9a9
  }

  .row {
  	display: grid;
  	grid-template-columns: 1fr 1fr 1fr 1fr;
  	grid-row-gap: 1rem;
  	margin-top: 2rem;
  	margin-bottom: 2rem
  }

  @media screen and (min-width: 1200px) {
  	.row {
  		padding: 0 5rem
  	}
  }

  .row img {
  	max-width: 100%;
  	height: auto;
  	display: block;
  	border-top-left-radius: 6px;
  	border-top-right-radius: 6px;
  	padding: 6px
  }

  .one-fourth-column .product-info {
  	box-shadow: 0 0 2px #0003
  }

  .one-fourth-column .product-info .product_grid_items {
  	padding: 0 6px
  }

  @media (max-width: 749px) {
  	.card-information__text a {
  		min-height: 20px !important
  	}

  	.add-to-box,
  	.add-to-box input:disabled+span {
  		font-size: 1.2rem !important
  	}

  	.collection_info h3 {
  		font-size: 18px !important
  	}

  	.sticky-header {
  		min-height: 60px !important;
  		top: 85px !important
  	}
  }

  @media screen and (max-width: 989px) {
  	.row {
  		grid-template-columns: 1fr 1fr;
  		grid-column-gap: 2rem;
  		grid-row-gap: 2rem
  	}
  }

  .add-to-box {
  	background-color: #171615;
  	min-width: 115px;
  	width: 100%;
  	font-size: 1.3rem;
  	font-weight: 600;
  	color: #fff;
  	margin: 5px 0
  }

  .add-to-box label span {
  	text-align: center;
  	padding: 8px;
  	display: block;
  	cursor: pointer
  }

  .add-to-box label input {
  	position: absolute;
  	display: none;
  	color: #212326 !important
  }

  .add-to-box label input+span {
  	color: #fff;
  	font: inherit;
  	font-family: inherit;
  	letter-spacing: .2rem
  }

  .add-to-box input:checked+span {
  	color: #fff
  }

  .add-to-box input:disabled+span {
  	cursor: not-allowed;
  	background-color: #a3aea5;
  	min-width: 115px;
  	width: 100%;
  	font-size: 1.3rem;
  	font-weight: 600;
  	color: #fff
  }

  .add-to-box input:checked+span:hover {
  	background-color: transparent
  }

  input[type=checkbox]+span:after {
  	content: "ADD TO BOX"
  }

  input[type=checkbox]:checked+span:after {
  	content: "REMOVE ITEM"
  }

  div.product-info:has(input[type=checkbox]:checked) {
  	background-color: #75757521
  }

  @media (max-width: 767px) {
  	.product_grid_items h3 a {
