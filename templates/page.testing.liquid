{%- for collection in collections -%}
  {% if collection.title == 'Trapo Classic' %}
    {% paginate collection.products by 600 %}
      {%- for product in collection.products -%}
        {% if product.type == 'TRAPO CLASSIC' %}
          {% for tag in product.tags %}
            {% if tag contains 'brand:' %}
              {% assign brandName = tag | remove: 'brand:' %}
            {% endif %}
            {% if tag contains 'model:' %}
              {% assign modelName = tag | remove: 'model:' %}
            {% endif %}
            {% if tag contains 'year:' %}
              {{ brandName }}-{{ modelName }}-{{ tag | remove: 'year:' -}}
              <br>
            {% endif %}
          {%- endfor -%}
        {% endif %}
      {%- endfor -%}
    {% endpaginate %}
  {% endif %}
{%- endfor -%}
